{
    "name": "lexi",
    "version": "3.0.0",
    "description": "A Discord furry bot",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "start": "chmod u+x ./start.sh; ./start.sh",
        "lint": "eslint .",
        "build": "tsc; tsc-alias",
        "build:noalias": "tsc",
        "prettier:check": "prettier . --check",
        "prettier": "prettier . -w",
        "prepare": "husky install",
        "test": "yarn run lint; yarn run prettier:check; yarn run build",
        "test:staged": "npx lint-staged"
    },
    "author": "BadBoyHaloCat",
    "license": "GPL-3.0",
    "dependencies": {
        "@babel/core": "7.19.3",
        "@babel/eslint-parser": "^7.17.0",
        "@discordjs/rest": "^0.6.0",
        "@types/eslint": "^8.4.1",
        "acorn": "8.8.1",
        "better-sqlite3": "^7.5.1",
        "bytes": "^3.1.2",
        "cli-highlight": "^2.1.11",
        "colorette": "^2.0.16",
        "cosmiconfig": "7.0.1",
        "discord-api-types": "^0.37.0",
        "discord.js": "v14",
        "enmap": "^5.9.0",
        "eslint": "^8.13.0",
        "express": "^4.18.1",
        "markov-strings": "^3.0.1",
        "module-alias": "^2.2.2",
        "morgan": "^1.10.0",
        "node-fetch": "^3.2.3",
        "prettier": "^2.6.2",
        "source-map-support": "^0.5.21",
        "strip-ansi": "^7.0.1",
        "uwuifier": "^4.0.5"
    },
    "devDependencies": {
        "@types/bytes": "3.1.1",
        "@types/express": "4.17.14",
        "@types/module-alias": "2.0.1",
        "@types/morgan": "1.9.3",
        "@types/node": "17.0.30",
        "@types/node-fetch": "2.6.2",
        "@typescript-eslint/eslint-plugin": "5.40.1",
        "@typescript-eslint/parser": "5.40.1",
        "commitlint": "17.1.2",
        "conventional-changelog-conventionalcommits": "5.0.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-deprecation": "1.3.2",
        "eslint-plugin-license-header": "0.6.0",
        "eslint-plugin-simple-import-sort": "8.0.0",
        "eslint-plugin-unused-imports": "2.0.0",
        "husky": "8.0.1",
        "lint-staged": "13.0.3",
        "tsc-alias": "1.7.0",
        "typescript": "4.8.4"
    },
    "lint-staged": {
        "*.ts": [
            "eslint",
            "prettier --write"
        ],
        "*.js": [
            "eslint",
            "prettier --write"
        ],
        "*.md": [
            "prettier --write"
        ],
        "*.yml": [
            "prettier --write"
        ],
        "*.json": [
            "prettier --write"
        ]
    },
    "packageManager": "yarn@3.2.4"
}
