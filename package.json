{
    "name": "protobot",
    "version": "2.1.0",
    "description": "A Discord furry bot",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "start": "chmod u+x ./start.sh; ./start.sh",
        "lint": "eslint .",
        "build": "tsc; EX=\"$?\"; tsc-alias; echo \"$EX\"; [[ \"$EX\" == 0 ]] && false || true",
        "prettier:check": "prettier . --check",
        "prettier": "prettier . -w",
        "prepare": "husky install",
        "test": "npm run lint; npm run prettier:check; npm run build",
        "test:staged": "npx lint-staged"
    },
    "author": "BadBoyHaloCat",
    "license": "GPL-3.0",
    "dependencies": {
        "@babel/eslint-parser": "^7.17.0",
        "@types/eslint": "^8.4.1",
        "better-sqlite3": "^7.5.1",
        "bytes": "^3.1.2",
        "chalk": "^5.0.1",
        "cli-highlight": "^2.1.11",
        "discord.js": "^13.6.0",
        "enmap": "^5.9.0",
        "markov-strings": "^3.0.1",
        "module-alias": "^2.2.2",
        "node-fetch": "^3.2.3",
        "prettier": "^2.6.2",
        "source-map-support": "^0.5.21",
        "strip-ansi": "^7.0.1",
        "eslint": "^8.13.0",
        "uwuifier": "^4.0.5"
    },
    "devDependencies": {
        "@types/bytes": "3.1.1",
        "@types/module-alias": "2.0.1",
        "@types/node": "17.0.30",
        "@types/node-fetch": "2.6.1",
        "@typescript-eslint/eslint-plugin": "5.22.0",
        "@typescript-eslint/parser": "5.22.0",
        "commitlint": "16.3.0",
        "conventional-changelog-conventionalcommits": "5.0.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-deprecation": "1.3.2",
        "eslint-plugin-license-header": "0.4.0",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "eslint-plugin-unused-imports": "2.0.0",
        "husky": "7.0.4",
        "lint-staged": "12.4.2",
        "tsc-alias": "1.6.7",
        "typescript": "4.7.2"
    },
    "lint-staged": {
        "*.ts": [
            "eslint",
            "prettier --write"
        ],
        "*.js": [
            "eslint",
            "prettier --write"
        ],
        "*.md": [
            "prettier --write"
        ],
        "*.yml": [
            "prettier --write"
        ],
        "*.json": [
            "prettier --write"
        ]
    }
}
