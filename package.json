{
    "name": "protobot",
    "version": "2.1.0",
    "description": "A Discord furry bot",
    "main": "dist/index.js",
    "scripts": {
        "start": "chmod u+x ./start.sh; ./start.sh",
        "lint": "eslint .",
        "build": "tsc",
        "prettier:check": "prettier . --check",
        "prettier": "prettier . -w",
        "prepare": "husky install",
        "test": "npm run lint && npm run prettier:check && npm run build",
        "test:staged": "npx lint-staged"
    },
    "author": "BadBoyHaloCat",
    "license": "GPL-3.0",
    "dependencies": {
        "better-sqlite3": "^7.5.1",
        "bytes": "^3.1.2",
        "chalk": "^4.1.2",
        "cli-highlight": "^2.1.11",
        "discord.js": "^13.6.0",
        "enmap": "^5.8.13",
        "markov-strings": "^3.0.1",
        "module-alias": "^2.2.2",
        "node-fetch": "^2.6.7",
        "prettier": "^2.6.2",
        "source-map-support": "^0.5.21",
        "strip-ansi": "^6.0.0",
        "uwuifier": "^4.0.5"
    },
    "devDependencies": {
        "@types/bytes": "^3.1.1",
        "@types/module-alias": "^2.0.1",
        "@types/node": "^17.0.23",
        "@types/node-fetch": "^2.5.11",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "babel-eslint": "^10.1.0",
        "commitlint": "^16.2.3",
        "conventional-changelog-conventionalcommits": "^4.6.3",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-deprecation": "^1.3.2",
        "husky": "^7.0.0",
        "lint-staged": "^12.3.7",
        "typescript": "^4.6.3"
    },
    "lint-staged": {
        "*.ts": [
            "eslint",
            "prettier --write"
        ],
        "*.js": [
            "eslint",
            "prettier --write"
        ],
        "*.md": [
            "prettier --write"
        ],
        "*.yml": [
            "prettier --write"
        ],
        "*.json": [
            "prettier --write"
        ]
    }
}
